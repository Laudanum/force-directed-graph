{
  "name": "onlinevenue",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-task:scss-compile": "node-sass --source-map true src/scss/ -o dist/assets/css",
    "build-task:copy": "cp -a src/public/* dist/ && cp -a src/assets/ dist/assets",
    "build-task:js-compile": "WEBPACK_ENV=dev webpack",
    "build-all": "mkdir -p dist/assets && npm-run-all --parallel build-task:*",
    "build-records": "mkdir -p dist/record && node utils/template-compile assets/data/data.json src/templates dist/record",
    "serve": "browser-sync start --server './dist' --files './dist/**/*' --watch --index 'index.html' --startPath '/index.html'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-watch build-all",
    "watch": "npm-watch build-all"
  },
  "watch": {
    "build-all": {
      "patterns": [
        "src"
      ],
      "extensions": "scss,js",
      "ignore": "*.min.js",
      "quiet": false,
      "legacyWatch": true,
      "delay": 1500,
      "runOnChangeOnly": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Laudanum/force-directed-graph.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Laudanum/force-directed-graph/issues"
  },
  "homepage": "https://github.com/Laudanum/force-directed-graph#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "acorn": "^7.1.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.26.7",
    "d3-force": "^2.0.1",
    "d3-selection": "^1.4.1",
    "handlebars": "^4.5.3",
    "handlebars-helpers": "^0.10.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
